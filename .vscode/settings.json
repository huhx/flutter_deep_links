{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        ".vscode": true,
        "**/.idea": true,
        "**/.dart_tool": true,
        "build": true,
        ".flutter-plugins": true,
        ".flutter-plugins-dependencies": true,
        "symbols": true,
        "**/*.g.dart": true,
        "android/.gradle": true,
        "ios/.symlinks": true,
        "ios/Pods": true,
    },
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontSize": 12,
    "dart.closingLabels": false,
}